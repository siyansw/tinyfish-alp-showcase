{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "frontend/static/$1"
    },
    {
      "src": "/loading",
      "dest": "frontend/loading.html"
    },
    {
      "src": "/dashboard",
      "dest": "frontend/index.html"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    }
  ]
}
